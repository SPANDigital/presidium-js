    "copy-webpack-plugin": "^4.1.0",
    "cpx": "^1.5.0",
    "eslint": "^1.10.3",
    "eslint-plugin-react": "^3.6.2",
    "mocha": "^3.2.0",
    "moxios": "^0.4.0",
    "react": "^15.4.1",
    "react-addons-update": "^15.4.1",
